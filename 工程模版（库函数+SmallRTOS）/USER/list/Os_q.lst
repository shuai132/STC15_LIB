              ********************************************************************************************************/
              
                      uint8 OSQCreate(uint8 OS_Q_MEM_SEL *Buf, uint8 SizeOfBuf)
              //Buf[0]:队列中字节数,Buf[1]:Buf总长度,Buf[2]:出对端,Buf[3](,Buf[4]):等待队列任务列表
              {
                  OS_ENTER_CRITICAL();
              
                  if ((SizeOfBuf >= 5) && (Buf != NULL))
                  {
                      Buf[0] = 0;                         /* 队列中消息数目           */
                      Buf[1] = SizeOfBuf;                 /* 消息队列占用内存字节数    */
              #if OS_MAX_TASKS < 9
                      Buf[2] = 4;                         /* 将要出队的消息所在位置    */
              #else
                      Buf[2] = 5;                         /* 将要出队的消息所在位置    */
              #endif
                      Buf[3] = 0;                         /* 消息队列的等待任务列表    */
                      Buf[4] = 0;                         /* 任务数大于等于8时为等待任务列表的一部分， */
                                                          /* 否则为消息缓冲区         */
              
                      OS_EXIT_CRITICAL();
                      return OS_Q_OK;
                  }
                  else
                  {
                      OS_EXIT_CRITICAL();
                      return NOT_OK;
                  }
              
              }
              
